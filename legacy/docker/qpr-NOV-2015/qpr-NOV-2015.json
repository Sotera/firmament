[
  {
    "name": "datawake-mongo",
    "Image": "jreeme/qpr-nov2015-mongo:2.6",
    "DockerFilePath": "docker/qpr-NOV-2015/mongo/2.6",
    "Hostname": "mongo",
    "ExposedPorts": {
      "27017/tcp": {}
    },
    "HostConfig": {
    }
  },
  {
    "name": "datawake-depot",
    "Image": "jreeme/strongloop:10",
    "DockerFilePath": "docker/strong-pm",
    "Hostname": "datawake",
    "ExposedPorts": {
      "3001/tcp": {},
      "8701/tcp": {}
    },
    "HostConfig": {
      "Links": [
        "datawake-mongo:mongo"
      ],
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "8082"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8702"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/DatawakeDepot",
        "GitSrcBranchName": "qpr-nov-2015",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8702",
        "ServiceName": "DatawakeDepot",
        "Scripts": [
          {
            "RelativeWorkingDir": ".",
            "Command": "bower",
            "Args": [
              "install",
              "--config.interactive=false"
            ]
          }
        ]
      }
    ]
  }
]
